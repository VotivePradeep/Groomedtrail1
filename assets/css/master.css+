    top: 2px;
    background: #fff;
    left: 0;
    margin: 0;
}
.main-chk label:after {
    opacity: 1;
    content: '';
    position: absolute;
    width: 15px;
    height: 15px;
    background: transparent;
    background: url('../images/un-checked.png');
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin: 0;
}
.main-chk label:hover::after {
    opacity: 0.3;
}
.main-chk input[type=checkbox]:checked + label:after {
    opacity: 1;
    background: url('../images/checkd.png');
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin: 0;
    width: 15px;
    height: 15px;
}




