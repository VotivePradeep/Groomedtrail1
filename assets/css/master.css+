body {
    font-family: open sans, sans-serif;
    overflow-x: hidden;
}
*:focus,
a:focus,
button:focus,
input:focus {
    outline: none !important;
    box-shadow: none !important;
}
/**nav**/
section {
    overflow-x: hidden;
}
ul.nav.navbar-nav {
    display: inline-block;
    float: right;
    margin: 0 0 -15px;
}
ul.nav.navbar-nav li {
    list-style: none;
    padding: 0 0;
    margin: 0;
}
ul.nav.navbar-nav li a:hover,
ul.nav.navbar-nav li.active a {
    background: transparent;
}
ul.nav.navbar-nav li a:focus {
    background: transparent;
}
a.navbar-brand {
    padding: 0;
    height: inherit;
}
ul.select-my-profile li {
    list-style: none;
    padding: 0 0;
    margin: 0;
    display: inline-block;
    margin-left: -4px;
}
.main-header ul.navbar-nav li.active a {
    border-bottom: 2px solid #000 !important;
    color: #387fcc !important;
}
li.txtcountrysearch {
    padding: 0;
    list-style: none;
    color: #000;
    width: 100%;
    display: block;
    border-top: 1px solid #e6e6e6;
}
li.txtcountrysearch a {
    color: #000;
    padding: 5px 25px;
    display: block;
}
div#map {
    height: calc(100vh - 110px);
    height: -webkit-calc(100vh - 110px);
    height: -moz-calc(100vh - 110px);
    width: 100%;
    position: relative;
    overflow: hidden;
    display: inline-block;
}
.side-user-actvi {
    width: 100%;
    overflow: hidden;
    padding: 15px;
    height: 100%;
}
.side-user-actvi.mCustomScrollbar .mCSB_inside>.mCSB_container {
    margin-right: 20px;
}
.side-user-actvi.mCustomScrollbar .mCSB_scrollTools .mCSB_draggerRail {
    background-color: rgba(0, 0, 0, 0.15) !important;
    filter: "alpha(opacity=0.15)";
    -ms-filter: "alpha(opacity=0.15)";
}
.side-user-actvi.mCustomScrollbar .mCSB_scrollTools .mCSB_draggerRail {
    width: 12px !important;
}
.sidenav2 {
    position: fixed;
    width: 320px;
    right: -15px;
    z-index: 88;
    top: 137px;
    height: 80vh;
    padding-top: 0;
    background: rgba(245, 245, 245, 0.85);
    transition: all ease-in-out 400ms;
    -webkit-transition: all ease-in-out 400ms;
    -moz-transition: all ease-in-out 400ms;
    -ms-transition: all ease-in-out 400ms;
    -o-transition: all ease-in-out 400ms;
}
.trail_type_cls ul {
    padding: 0;
    display: block;
    margin: 9px 0 3px;
    overflow: hidden;
}
.trail_type_cls ul li {
    display: inline-block;
    padding: 0;
    margin: 0;
    float: left;
    width: 50%;
}
.trail_type_cls ul li a {
    display: block;
    padding: 8px 11px;
    color: #000;
    background: #fff;
    border-bottom: none;
    font-size: 15px;
    font-weight: 600;
    text-align: center;
}
.trail_type_cls ul li.active a {
    background: #387fcc;
    color: #fff;
}
span#open-nav-id2, #clse-nav {
    position: absolute;
    left: -30px;
    top: 50%;
    width: 30px;
    height: 120px;
    text-align: center;
    background: rgba(56, 127, 204, 0.91);
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    color: #fff;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
}
span#open-nav-id2 article, #clse-nav article {
    transform: rotate(-90Deg);
    width: 100px;
    top: 40px;
    height: 87px;
    font-weight: 600;
}
span#open-nav-id2 .fa, #clse-nav .fa {
    width: 26px;
    background: #fff;
    height: 26px;
    color: #387fcc;
    border-radius: 5px 0 0 0;
    padding: 6px;
    margin-top: 2px;
}
.button-opn-cls label {
    max-width: 80px;
    width: 80px;
    font-size: 14px;
    color: #fff;
    font-weight: 600;
    transform: rotate(-90Deg);
    margin: 26px 0 0 -25px;
    height: 0px;
    cursor: pointer;
}
.button-opn-cls .fa {
    font-size: 15px;
}
ul.select-my-profile {
    display: block;
    float: right;
    padding: 0;
    margin: 0 -30px 0 0;
    position: relative;
}
.search-form input.btn.search-btn {
    position: absolute;
    top: 0;
    right: 0;
    margin-top: 5px;
    height: 38px;
    background: #000;
    border: none;
    border-radius: 0;
    color: #fff;
    text-align: center;
    text-transform: uppercase;
}
.search-form input.seach-in {
    height: 40px;
    width: 100%;
    margin-top: 4px;
    padding: 8px 10px;
}
.search-form {
    position: absolute;
    right: 0;
    width: 100%;
    z-index: 9999;
}
.top-nav ul {
    padding: 0;
    margin: 0 -20px 0 0;
    display: block;
    text-align: right;
}
.top-nav ul li {
    display: inline-block;
    list-style: none;
}
.top-nav ul li a:hover {
    text-decoration: none;
}
.top-header {
    padding: 0 15px;
}
.main-header {
    padding: 0 6px;
}
/*****/
section.map-sec {
    position: relative;
    overflow: hidden;
}
.map-sec iframe {
    height: 100vh;
    width: 100%;
}
div.info-box {
    position: fixed;
    right: 0;
    bottom: 0;
    max-width: 300px;
    width: 100%;
    height: 488px;
    overflow: hidden;
