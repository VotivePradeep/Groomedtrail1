    background: #e6e6e6;
    border-radius: 0px;
    border: none;
    height: 40px;
    box-shadow: none;
    margin-bottom: 25px !important;
}
input.form-control:focus,
textarea.form-control:focus {
    box-shadow: none;
}
textarea.form-control {
    background: #e6e6e6;
    border: none;
    box-shadow: none;
    margin-bottom: 20px !important;
    width: 100% !important;
    overflow: hidden;
    border-radius: 0px;
    line-height: 25px;
