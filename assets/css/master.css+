}
.btn:focus,
button:focus,
input:focus,
a:focus {
    outline: none;
}
/**footer**/
.f_heading ul li a {
    text-transform: uppercase;
    color: #fff;
    display: block;
    padding: 3px 0;
    font-size: 13px;
}
.new-letter p {
    padding: 0 25px 0 0;
    color: #fff;
    line-height: 25px;
}
.f_heading ul li {
    display: block;
    padding: 0;
    list-style: none;
}
.f_heading ul {
    display: block;
    margin: 0 auto;
    padding: 0;
    text-align: left;
}
.f_heading {
    text-align: left;
    width: 100%;
}
.f_heading ul li a:hover {
    text-decoration: none;
}
footer.main_footer {
    background: #000;
    padding: 30px 0;
}
.copy-right {
    background: #454343;
    padding: 24px 0;
}
.c-right {
    color: #fff;
    font-size: 13px;
}
.social-nav ul {
    display: block;
    text-align: right;
    margin: 0;
    padding: 0;
}
.social-nav ul li {
    display: inline-block;
    padding: 0 10px;
    margin: 0;
}
.new-fild {
    position: relative;
}
.new-fild input {
    height: 45px;
    padding: 10px 105px 10px 10px;
    width: 100%;
    border: none;
    color: #fff;
}
.new-fild button {
    position: absolute;
    top: 0;
    height: 45px;
    right: 0;
    border: 0;
    padding: 14px;
    border-radius: 0;
}
.social-nav ul li a {
    display: block;
    margin: 0;
}
.social-nav ul li a .fa {
    color: #000;
    font-size: 18px;
}
.main_footer h2 {
    margin: 0 0 5px;
    padding: 0;
    font-size: 20px;
    font-weight: 600;
}
.social-nav ul li a:hover .fa {
    color: #fff;
}
.box-content p {
    text-align: justify;
}
/*My News*/
section.bread-crumbs {
    position: relative;
    background: url(../images/img.jpg);
    height: 250px;
    background-attachment: fixed;
    background-position: top;
    background-size: 100%;
}
