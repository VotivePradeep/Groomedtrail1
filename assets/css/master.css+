    position: relative;
    z-index: 10;
}
.bread-crumbs-sub h3 {
    font-weight: 700;
    font-size: 30px;
    color: #fff;
}
section.bread-crumbs::before {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.6);
    content: '';
    z-index: 0;
}
.news-img {
    overflow: hidden;
    height: 400px;
}
.news-img img {
    width: 100%;
    min-height: 100%;
}
.owl-wrapper-outer {
    background: #292929;
}
section.my-news-sec {
    padding: 25px 0px;
}
.news-txt {
    color: #fff;
}
.news-txt h2 {
    font-weight: 200;
    text-transform: capitalize;
    font-size: 22px;
    color: #b3b3b3;
    letter-spacing: 0.3px;
    line-height: 30px;
    max-height: 90px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}
.news-txt p {
    letter-spacing: 0.3px;
    line-height: 23px;
    max-height: 114px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 5;
}
.news-txt {
    padding-right: 25px;
}
h3.evnt-title {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
